{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "## Getting Started",
  "main": "server.ts",
  "scripts": {
    "start": " prettier --write \"src/**/*.ts\" && nodemon --exec npx ts-node src/index.ts",
    "watch": "tsc-watch --esModuleInterop src/index.ts --outDir ./build",
    "devUp": "db-migrate up --config ./database.json --e dev",
    "devDown": "db-migrate reset --config ./database.json --e dev",
    "test": " set ENV=test && tsc && db-migrate up --env test && tsc && jasmine && db-migrate reset",
    "build": "npx tsc",
    "prettier": "prettier --write \"src/**/*.ts\"",
    "tsc": "tsc"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "dotenv": "^16.0.0",
    "express": "^4.18.1",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/db-migrate-pg": "^0.0.10",
    "@types/dotenv": "^8.2.0",
    "@types/jasmine": "^4.0.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/nodemon": "^1.19.1",
    "@types/pg": "^7.14.11",
    "@types/prettier": "^2.6.1",
    "@types/supertest": "^2.0.12",
    "bcrypt": "^5.0.1",
    "db-migrate-pg": "^1.2.2",
    "jasmine": "^4.1.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.3.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.16",
    "pg": "^8.7.3",
    "prettier": "^2.6.2",
    "supertest": "^6.2.3",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9"
  },
  "keywords": []
}
